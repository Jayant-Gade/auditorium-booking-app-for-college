{
  "expo": {
    "name": "YCCE Auditorium Booking",
    "slug": "ycce-auditorium-booking",
    "origin": "http://localhost:8081",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "description": "Official auditorium booking app for Yeshwantrao Chavan College of Engineering, Nagpur",
    "primaryColor": "#1565C0",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1565C0"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": "auditoriumbooker",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ycce.auditorium.booking"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1565C0"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.ycce.auditorium.booking",
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR"
      ],
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "ycce-auditorium-booking"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#ffffff"
        }
      ],
      "expo-secure-store",
      "expo-router"
    ]
  }
}
